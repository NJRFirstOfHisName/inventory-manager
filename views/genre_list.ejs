<%- include('layout', {title:title}) %>

<div class="content">
  <h1><%= title %></h1>
  <ul>
    <% genre_list.forEach(genre => { %>
    <li>
      <a href="<%= genre.url %>"><strong><%= genre.name %></strong></a> (<em
        ><%= genre.description %></em
      >)
    </li>
    <% }) %> <% if(!genre_list) {%>
    <li>There are no genres.</li>
    <% } %>
  </ul>
</div>
